@page "{id:int}"
@model SysJaky_N.Pages.Admin.Orders.EditModel
@{
    ViewData["Title"] = "Změna stavu objednávky";
}

<h1>Změna stavu objednávky</h1>

<form method="post">
    <div class="form-group">
        <label asp-for="Order.Status"></label>
        <select asp-for="Order.Status" asp-items="Html.GetEnumSelectList<OrderStatus>()"></select>
    </div>
    <button type="submit">Uložit</button>
</form>
