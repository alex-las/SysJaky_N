@page
@model SysJaky_N.Pages.Admin.AuditLogs.IndexModel
@{
    ViewData["Title"] = "Auditní záznamy";
}

<h1>Auditní záznamy</h1>
<table class="table">
    <thead>
        <tr>
            <th>Uživatel</th>
            <th>Akce</th>
            <th>Čas</th>
            <th>Data</th>
        </tr>
    </thead>
    <tbody>
    @foreach (var log in Model.Logs)
    {
        <tr>
            <td>@log.UserId</td>
            <td>@log.Action</td>
            <td>@log.Timestamp</td>
            <td>@log.Data</td>
        </tr>
    }
    </tbody>
</table>

